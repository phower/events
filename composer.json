{
    "name": "phower/events",
    "description": "Events oriented programming for PHP.",
    "type": "library",
    "keywords": [
        "php",
        "phower",
        "events"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "phower/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.8",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Events\\": "tests/"
        }
    },
